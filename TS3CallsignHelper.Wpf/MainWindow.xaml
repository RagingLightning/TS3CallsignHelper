<Window x:Class="TS3CallsignHelper.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vms="clr-namespace:TS3CallsignHelper.Wpf.ViewModels"
        xmlns:views="clr-namespace:TS3CallsignHelper.Wpf.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <DataTemplate DataType="{x:Type vms:CanvasContainerViewModel}">
            <views:CanvasContainerView/>
        </DataTemplate>
    </Window.Resources>
    <DockPanel>
        <Canvas x:Name="uiBase">
            
        </Canvas>
    </DockPanel>
</Window>
